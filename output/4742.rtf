\par }\pard\plain \ltrpar\s18\ql \li0\ri0\sb100\sa100\sbauto1\saauto1\widctlpar\wrapdefault\aspalpha\aspnum\faauto\adjustright\rin0\lin0\itap0\pararsid10767046 \rtlch \af0\afs24\alang1037 \ltrch \fs24\lang2057\langfe1033\cgrid\langnp2057\langfenp1033 {\rtlch 
